<div class="container list_creator" style>
  <div class="header">
    <h1>WatchList Creator</h1>
  </div>
  <div class="movies_list">
  <h2>Movies Lists</h2>
    <ul class="list-cards">
      <% @lists.each do |list| %>
        <li>
          <%= link_to list.name, list_path(list) %>
        </li>
      <% end %>
          <%= link_to 'add list', new_list_path, :class => "btn btn-flat mt-5 rounded-pill"%>
    </ul>
  </div>
</div>
